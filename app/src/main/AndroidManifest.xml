<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfirestore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SubCollectionActivity"></activity>
        <activity android:name=".NestedObjectsActivity" />
        <activity android:name=".ArraysActivity" />
        <activity android:name=".TransactionsActivity" />
        <activity android:name=".BatchWriteActivity" />
        <activity android:name=".DocumentChangesActivity" />
        <activity android:name=".PaginationActivity" />
        <activity android:name=".MergingTask" />
        <activity android:name=".MultipleDocument" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>